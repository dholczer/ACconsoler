<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net452;net5.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
    <Authors>D. Holczer</Authors>
    <Company />
    <Description>This is a fork from Nconsoler library for parsing console arguments in C#. I migrated this library to fit my client's needs. Compatibility with Microsoft .NET Framework 4.5.2 to .NET 5.0</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/dholczer/ACconsoler</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dholczer/ACconsoler</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageId>AConsoler</PackageId>
    <Product>AConsoler</Product>
  </PropertyGroup>

